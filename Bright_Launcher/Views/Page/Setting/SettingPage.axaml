<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="Bright_Launcher.Views.Pages.Setting.SettingPage">
	<Grid>
		<Grid.RowDefinitions>
			<RowDefinition/>
			<RowDefinition Height="80"/>
		</Grid.RowDefinitions>
		<Border CornerRadius="10" Margin="20 20 20 20" BoxShadow="0 2 20 0 #66000000" Background="#DBDBDB" Grid.Row="0">
			<ContentControl x:Name="FrameX"/>
		</Border>
		<Border BoxShadow="0 2 20 0 #66000000" Background="#DBDBDB" Grid.Row="1">
			
		</Border>
	</Grid>
</UserControl>
